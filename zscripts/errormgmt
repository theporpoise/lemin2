#!/usr/bin/
echo "\nNO ROOM"
make ; sleep .1 ;  ./lem-in < ztestmaps/norooms.map
echo "\nNO ANTS"
make ; sleep .1 ;  ./lem-in < ztestmaps/noants.map
echo "\nNO START"
make ; sleep .1 ;  ./lem-in < ztestmaps/nostart.map
echo "\nNO END"
make ; sleep .1 ;  ./lem-in < ztestmaps/noend.map
echo "\nNO START OR END"
make ; sleep .1 ;  ./lem-in < ztestmaps/nostartorend.map
echo "\nNO SOLUTION"
make ; sleep .1 ;  ./lem-in < ztestmaps/nosolution.map
echo "\nANTS > INT_MAX"
make ; sleep .1 ;  ./lem-in < ztestmaps/noants.map
echo "\nRM LNK NOT EXIST"
make ; sleep .1 ;  ./lem-in < ztestmaps/rmlnknotexist.map
echo "\nZERO ANTS"
make ; sleep .1 ;  ./lem-in < ztestmaps/zeroants.map
echo "\nRM LNK NOT EXIST"
make ; sleep .1 ;  ./lem-in < ztestmaps/rmlnknotexist1.map
echo "\nRM NAME WITH HYPHEN"
make ; sleep .1 ;  ./lem-in < ztestmaps/rmbadname.map
echo "\nONE ROOM"
make ; sleep .1 ;  ./lem-in < ztestmaps/oneroom.map
echo "\nONLY START AND END - THIS WORKS WITH NO ERROR"
make ; sleep .1 ;  ./lem-in < ztestmaps/onlystartandend.map
echo "\nNO LNKS"
make ; sleep .1 ;  ./lem-in < ztestmaps/nolnks.map
